generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  // posts Post[]
}

model Item {
  id      Int     @id @default(autoincrement())
  name    String?
}

